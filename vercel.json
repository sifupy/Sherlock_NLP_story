{
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "sherlock.settings"
    },
    "commands": [
      "pip install -r requirements.txt",
      "python manage.py collectstatic --noinput",
      "npm install",
      "npm run build"
    ]
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}