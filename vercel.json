{
  "version": 2,
  "builds": [
    {
      "src": "sherlock/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "postBuildCommand": "./build_files.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles_build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "sherlock/wsgi.py"
    }
  ]
}
